<%= simple_form_for(@print_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :job_request_id %>
    <%= f.input :position %>
    <%= f.input :block %>
    <%= f.input :color_count %>
    <%= f.input :print_method %>
    <%= f.input :attachment_data %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
